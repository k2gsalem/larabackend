SENTRY_PROFILES_SAMPLE_RATE=0.0

OPENAPI_SECURITY_SCHEME=BearerAuth
OPENAPI_BASE_PATH=/api

AWS_ECS_CLUSTER=
AWS_ECS_SERVICE=
AWS_ECS_REGION=

VITE_APP_NAME="${APP_NAME}"

GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI="${APP_URL}/auth/google/callback"
FACEBOOK_CLIENT_ID=
FACEBOOK_CLIENT_SECRET=
